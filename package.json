{
  "name": "cars-and-colors",
  "version": "1.0.0",
  "description": "This is a demo for an online car shop",
  "author": "Amir Ghadimi",
  "license": "MIT",
  "private": true,
  "homepage": "http://amirqadimi.github.io/cars-and-colors",
  "scripts": {
    "clean": "rimraf build/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.js ",
    "test": "./node_modules/.bin/eslint \"./src/**/*.?(js|jsx)\" ",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js --progress",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-fbjs": "^3.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.4.0",
    "postcss-loader": "^4.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.8.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1"
  }
}
